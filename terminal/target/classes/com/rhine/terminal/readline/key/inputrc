"\e[D": backward-char
"\e[C": forward-char
"\e[B": next-history
"\e[A": previous-history
"\C-?": backward-delete-char
"\C-h": backward-delete-char
"\C-X\C-U": undo
"\eb": backward-word
"\C-e": end-of-line
"\C-a": beginning-of-line
"\C-D": delete-char
"\e[3~": delete-char
"\C-i": complete
"\C-j": accept-line
"\C-m": accept-line
"\C-k": kill-line
"\eb": backward-word
"\ef": forward-word
"\e\C-?": backward-kill-word
